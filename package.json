{
  "name": "@poolfans/sdk",
  "version": "0.2.0",
  "description": "TypeScript SDK for deploying Clanker tokens with tokenized fee rewards via PoolFans",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./v4": {
      "types": "./dist/v4/index.d.ts",
      "import": "./dist/v4/index.mjs",
      "require": "./dist/v4/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/v4/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/v4/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "poolfans",
    "clanker",
    "tokenizer",
    "erc20",
    "revenue-share",
    "uniswap",
    "base",
    "ethereum",
    "sdk",
    "web3"
  ],
  "author": "PoolFans",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pool-fans/sdk"
  },
  "homepage": "https://pool.fans",
  "bugs": {
    "url": "https://github.com/pool-fans/sdk/issues"
  },
  "peerDependencies": {
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "viem": "^2.21.0",
    "vitest": "^2.1.0"
  }
}
